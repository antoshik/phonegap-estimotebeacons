<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
        id="com.example.estimote" version="0.9.9">

    <name>EstimoteBeacons</name>
    <description>EstimoteBeacons</description>
    <license>Apache 2.0</license>

    <js-module src="www/EstimoteBeacons.js" name="EstimoteBeacons">
        <clobbers target="EstimoteBeacons" />
    </js-module>
    <!-- android -->
    <platform name="android">
        <source-file src="src/android/EstimoteBeacons.java" target-dir="src/com/example/estimote" />
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="EstimoteBeacons">
                <param name="android-package" value="com.example.estimote" />
            </feature>
        </config-file>

    </platform>

</plugin>